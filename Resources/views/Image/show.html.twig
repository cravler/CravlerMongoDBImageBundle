
{% set dataAttr = '' %}
{% if data is defined %}
{% for attr, val in data %}
{% set dataAttr = dataAttr ~ ' data-' ~ attr ~ '="' ~ val ~ '"' %}
{% endfor %}
{% endif %}

<img src="{{ path('image_show', {'imageData': imageDataEncode(groupId, width, height, mode|default('inset'))}) }}{{ noCache|default(false) ? ':nocache' : '' }}"{{ id|default('') ? [' id="', id, '"']|join|raw : '' }}{{ class|default('') ? [' class="', class, '"']|join|raw : '' }}{{ style|default('') ? [' style="', style, '"']|join|raw : '' }} alt="{{ alt|default('') }}"{{ dataAttr|raw }} />
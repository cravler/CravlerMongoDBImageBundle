
{% set dataAttr = '' %}
{% if data is defined %}
{% for attr, val in data %}
{% set dataAttr = dataAttr ~ ' data-' ~ attr ~ '="' ~ val ~ '"' %}
{% endfor %}
{% endif %}
{% set imageData = imageDataEncode(groupId, width, height, mode|default('inset')) %}
<img src="{{ path('image_show', {'f1': imageF1(imageData), 'f2': imageF2(imageData), 'imageData': imageData}) }}{{ noCache|default(false) ? ':nocache' : '' }}"{{ id|default('') ? [' id="', id, '"']|join|raw : '' }}{{ class|default('') ? [' class="', class, '"']|join|raw : '' }}{{ style|default('') ? [' style="', style, '"']|join|raw : '' }} alt="{{ alt|default('') }}"{{ dataAttr|raw }} />